@article{Larsson2012,
abstract = {This paper introduces a matrix-valued analog of the Bessel processes, taking values in the set \$E\$ of real square matrices with nonnegative determinant. Focus is on existence and uniqueness, and especially the latter leads us to develop several new results of potential theoretic nature concerning the space of real square matrices. Specifically, the function \$w(x)=|\backslash det x|\^{}\backslash alpha\$ is a weight function in Muckenhoupt \$A\_p\$ class for \$-1<\backslash alpha\backslash le 0\$ (\$p=1\$) and \$-1<\backslash alpha<p-1\$ (\$p>1\$). The set of matrices of co-rank at least two has zero capacity with respect to the measure \$m(dx)=|\backslash det x|\^{}\backslash alpha dx\$ if \$\backslash alpha>-1\$, and if \$\backslash alpha\backslash ge 1\$ this even holds for the set of all singular matrices. As a consequence we obtain density results for Sobolev spaces over \$E\$ with Neumann boundary conditions. In obtaining these results we rely crucially on the stratification of the state space into smooth manifolds consisting of fixed-rank matrices. We also discuss the relation between the matrix-valued Bessel processes and the well-known Wishart processes.},
archivePrefix = {arXiv},
arxivId = {1212.4986},
author = {Larsson, Martin},
eprint = {1212.4986},
month = dec,
pages = {31},
title = {{Matrix-valued Bessel processes}},
url = {http://arxiv.org/abs/1212.4986},
year = {2012}
}
